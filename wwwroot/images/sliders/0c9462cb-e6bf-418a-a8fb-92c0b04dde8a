	<script>
		$(document).ready(function() {
			$("#SkillIds").select2({
				placeholder: "Tất cả kỹ năng",
				theme: "bootstrap4",
				allowClear: true
			});
			feather.replace();

			$("#tabs").tabs();

	
			const existingImages = [{"source":"/images/sliders/383395366_769786128284089_6737865291634600207_n.webp","options":{"type":"local","file":{"name":"383395366_769786128284089_6737865291634600207_n.webp","type":"image/png","size":123456},"metadata":{"id":1}}},{"source":"/images/sliders/350131531_624507389709008_9201446014570598061_n-min2.webp","options":{"type":"local","file":{"name":"350131531_624507389709008_9201446014570598061_n-min2.webp","type":"image/png","size":123456},"metadata":{"id":2}}},{"source":"/images/sliders/78906e46-5cd2-4eba-9b41-cebbcc8a127d.png","options":{"type":"local","file":{"name":"78906e46-5cd2-4eba-9b41-cebbcc8a127d.png","type":"image/png","size":123456},"metadata":{"id":16}}}];

			console.log("existingImages", existingImages)

			const pond = FilePond.create(document.querySelector('#images'), {
				allowMultiple: true,
				allowImagePreview: true,
				imagePreviewHeight: 140,
				server: {
					process: {
						url: '/images/upload',
						method: 'POST',
						withCredentials: false,
						onload: (response) => {
							const result = JSON.parse(response);
							return result.path; //FileName sẽ là key (source)
						}
					},
					revert: (uniqueFileId, load, error) => {
						fetch(`/images/revert?fileName=${encodeURIComponent(uniqueFileId)}`, {
								method: 'DELETE'
							})
							.then(res => {
								if (res.ok) {
									load();
								} else {
									error('Không thể xoá ảnh.');
								}
							});
					},
					load: (source, load, error, progress, abort, headers) => {
						console.log(source)
						const myRequest = new Request(source);
						fetch(myRequest).then(response => response.blob()).then(load);
					}
				},
				files: existingImages
			});

			// Lưu danh sách file đang có vào hidden input khi submit form
			const form = document.querySelector('form');
			form.addEventListener('submit', () => {
				const fileNames = pond.getFiles().map(f => f.serverId || f.source);
				document.getElementById('SavedImageFileNames').value = fileNames.join(',');
			});


		});
	</script>
	
<script>

    (function (factory) {
        if (typeof define === 'function' && define.amd) {
            define(['jquery'], factory);
        }
        else if (typeof module === 'object' && module.exports) {
            module.exports = factory(require('jquery'));
        } else {
            factory(window.jQuery);
        }
    }(function ($) {
        $.extend($.summernote.plugins, {
            // Tạo plugin tên elfinder
            'elfinder': function (context) {
                var self = this;
                // ui has renders to build ui elements.
                var ui = $.summernote.ui;
                // Tạo nút bấm
                context.memo('button.elfinder', function () {
                    var button = ui.button({
                        contents: '<i class="note-icon-picture"/> elFinder',
                        tooltip: 'Quản lý file',
                        click: function () {
                            // Bấm vào nút bấm gọi hàm elfinderDialog
                            elfinderDialog(context);
                        }
                    });
                    // create jQuery object from button instance.
                    var $elfinder = button.render();
                    return $elfinder;
                });
                // This methods will be called when editor is destroyed by $('..').summernote('destroy');
                // You should remove elements on `initialize`.
                this.destroy = function () {
                    this.$panel.remove();
                    this.$panel = null;
                };
            }

        });
    }));


    function elfinderDialog(editor) {
        var fm = $('<div/>').dialogelfinder({

            baseUrl: "/lib/elfinder/",
            url: "/file-manager-connector",

            lang: 'vi',
            width: 840,
            height: 450,
            destroyOnClose: true,
            getFileCallback: function (files, fm) {
                console.log(files);
                editor.invoke('editor.insertImage', files.url);
            },
            commandsOptions: {
                getfile: {
                    oncomplete: 'close',
                    folders: false
                }
            }
        }).dialogelfinder('instance');
    }



    $(document).ready(function () {
        $('#TopReason').summernote({
            height: 350,
            toolbar: [
                    ['style', ['style']],
                    ['font', ['bold', 'underline', 'clear']],
                    ['color', ['color']],
                    ['para', ['ul', 'ol', 'paragraph']],
                    ['table', ['table']],
                    ['insert', ['link', 'picture', 'video', 'elfinder']],
                    ['height', ['height']],
                    ['view', ['fullscreen', 'codeview', 'help']]
                ]
            });
    });
</script>
	
<script>

    (function (factory) {
        if (typeof define === 'function' && define.amd) {
            define(['jquery'], factory);
        }
        else if (typeof module === 'object' && module.exports) {
            module.exports = factory(require('jquery'));
        } else {
            factory(window.jQuery);
        }
    }(function ($) {
        $.extend($.summernote.plugins, {
            // Tạo plugin tên elfinder
            'elfinder': function (context) {
                var self = this;
                // ui has renders to build ui elements.
                var ui = $.summernote.ui;
                // Tạo nút bấm
                context.memo('button.elfinder', function () {
                    var button = ui.button({
                        contents: '<i class="note-icon-picture"/> elFinder',
                        tooltip: 'Quản lý file',
                        click: function () {
                            // Bấm vào nút bấm gọi hàm elfinderDialog
                            elfinderDialog(context);
                        }
                    });
                    // create jQuery object from button instance.
                    var $elfinder = button.render();
                    return $elfinder;
                });
                // This methods will be called when editor is destroyed by $('..').summernote('destroy');
                // You should remove elements on `initialize`.
                this.destroy = function () {
                    this.$panel.remove();
                    this.$panel = null;
                };
            }

        });
    }));


    function elfinderDialog(editor) {
        var fm = $('<div/>').dialogelfinder({

            baseUrl: "/lib/elfinder/",
            url: "/file-manager-connector",

            lang: 'vi',
            width: 840,
            height: 450,
            destroyOnClose: true,
            getFileCallback: function (files, fm) {
                console.log(files);
                editor.invoke('editor.insertImage', files.url);
            },
            commandsOptions: {
                getfile: {
                    oncomplete: 'close',
                    folders: false
                }
            }
        }).dialogelfinder('instance');
    }



    $(document).ready(function () {
        $('#Reason').summernote({
            height: 350,
            toolbar: [
                    ['style', ['style']],
                    ['font', ['bold', 'underline', 'clear']],
                    ['color', ['color']],
                    ['para', ['ul', 'ol', 'paragraph']],
                    ['table', ['table']],
                    ['insert', ['link', 'picture', 'video', 'elfinder']],
                    ['height', ['height']],
                    ['view', ['fullscreen', 'codeview', 'help']]
                ]
            });
    });
</script>
	
<script>

    (function (factory) {
        if (typeof define === 'function' && define.amd) {
            define(['jquery'], factory);
        }
        else if (typeof module === 'object' && module.exports) {
            module.exports = factory(require('jquery'));
        } else {
            factory(window.jQuery);
        }
    }(function ($) {
        $.extend($.summernote.plugins, {
            // Tạo plugin tên elfinder
            'elfinder': function (context) {
                var self = this;
                // ui has renders to build ui elements.
                var ui = $.summernote.ui;
                // Tạo nút bấm
                context.memo('button.elfinder', function () {
                    var button = ui.button({
                        contents: '<i class="note-icon-picture"/> elFinder',
                        tooltip: 'Quản lý file',
                        click: function () {
                            // Bấm vào nút bấm gọi hàm elfinderDialog
                            elfinderDialog(context);
                        }
                    });
                    // create jQuery object from button instance.
                    var $elfinder = button.render();
                    return $elfinder;
                });
                // This methods will be called when editor is destroyed by $('..').summernote('destroy');
                // You should remove elements on `initialize`.
                this.destroy = function () {
                    this.$panel.remove();
                    this.$panel = null;
                };
            }

        });
    }));


    function elfinderDialog(editor) {
        var fm = $('<div/>').dialogelfinder({

            baseUrl: "/lib/elfinder/",
            url: "/file-manager-connector",

            lang: 'vi',
            width: 840,
            height: 450,
            destroyOnClose: true,
            getFileCallback: function (files, fm) {
                console.log(files);
                editor.invoke('editor.insertImage', files.url);
            },
            commandsOptions: {
                getfile: {
                    oncomplete: 'close',
                    folders: false
                }
            }
        }).dialogelfinder('instance');
    }



    $(document).ready(function () {
        $('#Description').summernote({
            height: 350,
            toolbar: [
                    ['style', ['style']],
                    ['font', ['bold', 'underline', 'clear']],
                    ['color', ['color']],
                    ['para', ['ul', 'ol', 'paragraph']],
                    ['table', ['table']],
                    ['insert', ['link', 'picture', 'video', 'elfinder']],
                    ['height', ['height']],
                    ['view', ['fullscreen', 'codeview', 'help']]
                ]
            });
    });
</script>
	
