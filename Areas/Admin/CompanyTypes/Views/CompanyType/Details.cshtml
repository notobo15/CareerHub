@model RecruitmentApp.Models.CompanyType


<div class="row">
	<div class="col-md-12 grid-margin">
		<div class="card">
			<div class="card-body">
				<h4> 
					Detail
					<a asp-action="Details" asp-route-id="@Model.CompanyTypeId">#@Html.DisplayFor(model => model.CompanyTypeId)</a>
				</h4>
			</div>

		</div>
	</div>
</div>

<div class="row">
	<div class="col-md-12 grid-margin">
		<div class="card">
			<div class="card-body">
				<div class="card-title">
					 
				</div>
				<dl class="row">
					<dt class="col-sm-2">
						@Html.DisplayNameFor(model => model.Name)
					</dt>
					<dd class="col-sm-10">
						@Html.DisplayFor(model => model.Name)
					</dd>
				</dl>
			</div>

		

		</div>
	</div>
</div>


<div class="mt-4">
	
	<a asp-action="Index" class="btn btn-sm btn-outline-secondary d-inline-flex align-items-center">
		<i data-feather="arrow-left" class="mr-1 icon-md"></i> Back to List
	</a>
	<a asp-action="Edit" asp-route-id="@Model.CompanyTypeId" class="btn btn-sm btn-outline-warning d-inline-flex align-items-center  mx-2">
		<i data-feather="edit" class="mr-1 icon-md"></i> Edit
	</a>
	<a asp-action="Delete" asp-route-id="@Model.CompanyTypeId" class="btn btn-sm btn-outline-danger d-inline-flex align-items-center">
		<i data-feather="trash-2" class="mr-1 icon-md"></i> Delete
	</a>
</div>
