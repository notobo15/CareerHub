@model RecruitmentApp.Models.Title


<div class="row">
    
    <div class="col-md-6">
        
        <div>
            <a asp-action="Index"><i class="fas fa-chevron-left mr-2 p-2"></i>Back to List</a>
        </div>
        <h2 class="text-center">Tạo Mới</h2>
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Name" class="control-label"></label>
                <input asp-for="Name" class="form-control" />
                <span asp-validation-for="Name" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="p-2 w-100 btn btn-primary" />
            </div>
        </form>
    </div>
</div>

